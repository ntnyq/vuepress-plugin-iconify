{
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "iconify",
    "pathe",
    "pnpm",
    "tsbuildinfo",
    "vuepress"
  ],
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.formatOnSave": false
  }
}
